<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" xmlns:di="http://www.omg.org/spec/DD/20100524/DI" xmlns:camunda="http://camunda.org/schema/1.0/bpmn" id="Definitions_1" targetNamespace="http://bpmn.io/schema/bpmn" exporter="Camunda Modeler" exporterVersion="5.0.0">
  <bpmn:process id="job-recruitment-workflow-india" name="Job Recruitment Workflow India" isExecutable="true">
    
    <!-- Start Event -->
    <bpmn:startEvent id="StartEvent_1" name="Job Application Started">
      <bpmn:outgoing>Flow_1</bpmn:outgoing>
    </bpmn:startEvent>
    
    <!-- Step 1: Personal Information -->
    <bpmn:userTask id="PersonalInfoTask" name="Personal Information" camunda:formKey="personal-info">
      <bpmn:documentation>Collect basic personal details including name, email, mobile, date of birth, and gender</bpmn:documentation>
      <bpmn:incoming>Flow_1</bpmn:incoming>
      <bpmn:outgoing>Flow_2</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- Step 2: Job Preferences -->
    <bpmn:userTask id="JobPreferencesTask" name="Job Preferences" camunda:formKey="job-preferences">
      <bpmn:documentation>Collect job preferences including position, salary expectations, and notice period</bpmn:documentation>
      <bpmn:incoming>Flow_2</bpmn:incoming>
      <bpmn:outgoing>Flow_3</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- Step 3: Experience & Education -->
    <bpmn:userTask id="ExperienceEducationTask" name="Experience &amp; Education" camunda:formKey="experience-education">
      <bpmn:documentation>Collect professional experience and educational background including skills</bpmn:documentation>
      <bpmn:incoming>Flow_3</bpmn:incoming>
      <bpmn:outgoing>Flow_4</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- Validate Application -->
    <bpmn:serviceTask id="ValidateApplicationTask" name="Validate Application" camunda:class="com.dynamicworkflow.delegate.ValidationDelegate">
      <bpmn:documentation>Validate all application data and business rules</bpmn:documentation>
      <bpmn:incoming>Flow_4</bpmn:incoming>
      <bpmn:outgoing>Flow_5</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <!-- Validation Gateway -->
    <bpmn:exclusiveGateway id="ValidationGateway" name="Application Valid?">
      <bpmn:incoming>Flow_5</bpmn:incoming>
      <bpmn:outgoing>Flow_6</bpmn:outgoing>
      <bpmn:outgoing>Flow_7</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    
    <!-- Store Application Data -->
    <bpmn:serviceTask id="StoreApplicationTask" name="Store Application Data" camunda:class="com.dynamicworkflow.delegate.StoreApplicationDelegate">
      <bpmn:documentation>Store complete application data and update status to COMPLETED</bpmn:documentation>
      <bpmn:incoming>Flow_6</bpmn:incoming>
      <bpmn:outgoing>Flow_8</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <!-- Success End Event -->
    <bpmn:endEvent id="SuccessEndEvent" name="Application Submitted Successfully">
      <bpmn:incoming>Flow_8</bpmn:incoming>
    </bpmn:endEvent>
    
    <!-- Error End Event -->
    <bpmn:endEvent id="ErrorEndEvent" name="Application Validation Failed">
      <bpmn:incoming>Flow_7</bpmn:incoming>
    </bpmn:endEvent>
    
    <!-- Sequence Flows -->
    <bpmn:sequenceFlow id="Flow_1" sourceRef="StartEvent_1" targetRef="PersonalInfoTask" />
    <bpmn:sequenceFlow id="Flow_2" sourceRef="PersonalInfoTask" targetRef="JobPreferencesTask" />
    <bpmn:sequenceFlow id="Flow_3" sourceRef="JobPreferencesTask" targetRef="ExperienceEducationTask" />
    <bpmn:sequenceFlow id="Flow_4" sourceRef="ExperienceEducationTask" targetRef="ValidateApplicationTask" />
    <bpmn:sequenceFlow id="Flow_5" sourceRef="ValidateApplicationTask" targetRef="ValidationGateway" />
    <bpmn:sequenceFlow id="Flow_6" name="Valid" sourceRef="ValidationGateway" targetRef="StoreApplicationTask">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">${validationResult == true}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_7" name="Invalid" sourceRef="ValidationGateway" targetRef="ErrorEndEvent">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">${validationResult == false}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_8" sourceRef="StoreApplicationTask" targetRef="SuccessEndEvent" />
    
  </bpmn:process>
  
  <!-- BPMN Diagram Layout -->
  <bpmndi:BPMNDiagram id="BPMNDiagram_1">
    <bpmndi:BPMNPlane id="BPMNPlane_1" bpmnElement="job-recruitment-workflow-india">
      
      <!-- Start Event -->
      <bpmndi:BPMNShape id="_BPMNShape_StartEvent_2" bpmnElement="StartEvent_1">
        <dc:Bounds x="179" y="99" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="158" y="142" width="78" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <!-- Personal Info Task -->
      <bpmndi:BPMNShape id="PersonalInfo_Shape" bpmnElement="PersonalInfoTask">
        <dc:Bounds x="270" y="77" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <!-- Job Preferences Task -->
      <bpmndi:BPMNShape id="JobPreferences_Shape" bpmnElement="JobPreferencesTask">
        <dc:Bounds x="420" y="77" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <!-- Experience Task -->
      <bpmndi:BPMNShape id="Experience_Shape" bpmnElement="ExperienceEducationTask">
        <dc:Bounds x="570" y="77" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <!-- Validate Application -->
      <bpmndi:BPMNShape id="ValidateApplication_Shape" bpmnElement="ValidateApplicationTask">
        <dc:Bounds x="720" y="77" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <!-- Validation Gateway -->
      <bpmndi:BPMNShape id="ValidationGateway_Shape" bpmnElement="ValidationGateway" isMarkerVisible="true">
        <dc:Bounds x="875" y="92" width="50" height="50" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="858" y="62" width="84" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <!-- Store Application -->
      <bpmndi:BPMNShape id="StoreApplication_Shape" bpmnElement="StoreApplicationTask">
        <dc:Bounds x="980" y="77" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <!-- Success End Event -->
      <bpmndi:BPMNShape id="SuccessEnd_Shape" bpmnElement="SuccessEndEvent">
        <dc:Bounds x="1132" y="99" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="1111" y="142" width="78" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <!-- Error End Event -->
      <bpmndi:BPMNShape id="ErrorEnd_Shape" bpmnElement="ErrorEndEvent">
        <dc:Bounds x="882" y="202" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="861" y="245" width="78" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <!-- Sequence Flow Edges -->
      <bpmndi:BPMNEdge id="Flow_1_di" bpmnElement="Flow_1">
        <di:waypoint x="215" y="117" />
        <di:waypoint x="270" y="117" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_2_di" bpmnElement="Flow_2">
        <di:waypoint x="370" y="117" />
        <di:waypoint x="420" y="117" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_3_di" bpmnElement="Flow_3">
        <di:waypoint x="520" y="117" />
        <di:waypoint x="570" y="117" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_4_di" bpmnElement="Flow_4">
        <di:waypoint x="670" y="117" />
        <di:waypoint x="720" y="117" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_5_di" bpmnElement="Flow_5">
        <di:waypoint x="820" y="117" />
        <di:waypoint x="875" y="117" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_6_di" bpmnElement="Flow_6">
        <di:waypoint x="925" y="117" />
        <di:waypoint x="980" y="117" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="944" y="99" width="24" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_7_di" bpmnElement="Flow_7">
        <di:waypoint x="900" y="142" />
        <di:waypoint x="900" y="202" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="904" y="169" width="32" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_8_di" bpmnElement="Flow_8">
        <di:waypoint x="1080" y="117" />
        <di:waypoint x="1132" y="117" />
      </bpmndi:BPMNEdge>
      
    </bpmndi:BPMNPlane>
  </bpmndi:BPMNDiagram>
</bpmn:definitions>